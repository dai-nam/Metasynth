public class Offset
{
  public float xOff, yOff, zOff;
  public float rotX, rotY, rotZ;


  public Offset(float xo, float yo, float zo, float xr, float yr, float zr)
  {
    //Absolute Offset in Pixels
    xOff = xo;
    yOff = yo;
    zOff = zo;
    rotX = xr;
    rotY = yr;
    rotZ = zr;
  }
  
  
}
